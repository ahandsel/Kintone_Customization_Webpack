{
  "name": "kintone_customization_webpack",
  "version": "1.0.1",
  "description": "Template repo for a webpack project to be uploaded to Kintone as a customization",
  "main": "index.js",
  "scripts": {
    "start": "node scripts/npm-start.js",
    "upload": "env-cmd --verbose kintone-customize-uploader --watch dest/customize-manifest.json",
    "dev": "npm run build -- --watch",
    "build": "webpack",
    "production": "webpack --mode production"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "@kintone/customize-uploader": "^5.0.10",
    "babel-loader": "^8.3.0",
    "core-js": "^3.19.1",
    "css-loader": "^6.8.1",
    "env-cmd": "^10.1.0",
    "html-webpack-plugin": "^5.5.3",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "style-loader": "^3.3.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^4.10.0"
  }
}
